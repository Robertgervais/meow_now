<table class="table table-bordered table-striped">
		<tr>
			<th>User</td>
			<th>Membership</th>
			<th>Admin?</th>
		</tr>
	<% @users.each do |user| %>
		<tr>
			<% if current_user_is_admin %>
				<td><%= link_to "#{user.username}", edit_user_path(user) %></td>
			<% else %>
				<td><%= "#{user.username}" %></td>
			<% end %>
			<td><%= user.membership %></td>
			<% if user.admin %>
				<td>Yes</td>
			<% else %>
				<td>No</td>
			<% end %>
		</tr>
	<% end %>
</table>